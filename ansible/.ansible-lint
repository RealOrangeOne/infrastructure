skip_list:
  - command-instead-of-shell
  - no-handler
  - git-latest
  - fqcn
  - name[casing]
  - name[play]
  - no-changed-when
  - var-naming[no-role-prefix]

exclude_paths:
  - galaxy_roles/
  - galaxy_collections/
  - ~/.ansible
  - roles/nebula/files/nebula.yml
  - roles/traefik/files/traefik.yml
